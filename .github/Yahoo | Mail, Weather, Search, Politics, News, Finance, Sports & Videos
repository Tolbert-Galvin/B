.vp-ccs-container .vp-ccs-listitem .vp-ccs-toggle { border-radius: 7px; background-color: rgb(24, 143, 255); position: absolute; right: 13px; width: 28px; height: 14px; direction: rtl; margin-top: 12px; display: inline-block; }

