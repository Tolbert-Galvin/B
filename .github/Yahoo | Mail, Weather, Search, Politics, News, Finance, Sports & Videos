  0% { opacity: 0; }
  45% { opacity: 1; }
  55% { opacity: 1; }
  100% { opacity: 0; }
}

@-webkit-keyframes fade_in { 
  0% { opacity: 0; }
  100% { opacity: 1; }
}

@keyframes fade_in { 
  0% { opacity: 0; }
  100% { opacity: 1; }
}

